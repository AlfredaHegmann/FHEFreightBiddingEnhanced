  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy:local": "hardhat deploy --network localhost",
    "deploy:sepolia": "hardhat deploy --network fhevmSepolia",
